<?xml version="1.0" encoding="utf-8"?>
<pysysproject>
	<requires-pysys>2.1</requires-pysys>
	<requires-python>3</requires-python>

	<!-- Pre-defined properties include: ${testRootDir}, ${outDirName}, ${os}, ${osfamily}, ${startDate}, ${startTime}, 
		${hostname}, ${username}, ${/}, ${eval:*}, ${env:*}. 
	-->

	<property name="ANALYTICS_BUILDER_SDK" value="${env.ANALYTICS_BUILDER_SDK}"/>
	<property name="APAMA_HOME" value="${env.APAMA_HOME}"/>
	<property name="APAMA_WORK" value="${env.APAMA_WORK}"/>

	<property name="appHome" value="${env.PYSYS_APP_HOME}" default="${testRootDir}/blocks" pathMustExist="true"/>
	<property name="SOURCE" value="${testRootDir}"/>

	<property name="shutdownApamaComponentsAfterTest" value="true"/>

	<path value="${ANALYTICS_BUILDER_SDK}/testframework"/>

</pysysproject>
